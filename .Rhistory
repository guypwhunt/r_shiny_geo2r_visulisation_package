getGeoData <- function(geoAccessionCode, platform) {
gset <- getGEO(geoAccessionCode, GSEMatrix =TRUE, getGPL=FALSE)
if (length(gset) > 1) idx <- grep(platform, attr(gset, "names")) else idx <- 1
gset <- gset[[idx]]
return(gset)
}
